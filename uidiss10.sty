	\pagenumbering{roman}
	\setcounter{page}{2}  % PEK
	\pagestyle{plain}
	\singlespace
	\vspace*{\fill}
	\begin{quote}
	\begin{quote}
	\dedication
	\end{quote}
	\end{quote}
	\vspace*{\fill}
	}

\def\ackpage{
	\newpage
	\pagestyle{plain}			%3/14/98
	\doublespace
	\prefacesection{Acknowledgments}
	\indent
	\acknowledgement
        }

